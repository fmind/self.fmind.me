server {
    server_name _;
    listen 80 default_server;
    listen [::]:80 default_server;

    return 444;
}

server {
    server_name _;
    listen 443 default_server;
    listen [::]:443 default_server;

    ssl_certificate {{certbot_tls_cert}};
    ssl_certificate_key {{certbot_tls_cert_key}};

    return 444;
}
